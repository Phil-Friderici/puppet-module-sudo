{
  "name": "ericsson-sudo",
  "version": "0.8.0",
  "author": "ericsson",
  "summary": "manage sudo",
  "license": "Apache-2.0",
  "source": "git://github.com/Ericsson/puppet-module-sudo.git",
  "project_page": "https://github.com/Ericsson/puppet-module-sudo",
