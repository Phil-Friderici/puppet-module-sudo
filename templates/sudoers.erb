Defaults    <% unless @requiretty_real -%><%= "!" %><% end -%>requiretty
Defaults    <% unless @visiblepw_real -%><%= "!" %><% end -%>visiblepw
Defaults    <% unless @always_set_home_real -%><%= "!" %><% end -%>always_set_home
Defaults    <% unless @envreset_real -%><%= "!" %><% end -%>env_reset
<% if @envkeep -%>
Defaults    env_keep = "<%= @envkeep.join(" ") %>"
<% end -%>
Defaults    secure_path = <%= @secure_path %>
<% if @include_libsudo_vas_real -%>
Defaults    group_plugin="<%= @libsudo_vas_location_real %>"
<% end -%>
<% if @always_query_group_plugin_real -%>
Defaults    always_query_group_plugin
<% end -%>
<% if @root_allow_all_real -%>
root  ALL=(ALL)   ALL
<% end -%>
<% if @includedir_real -%>
## Read drop-in files from /etc/sudoers.d (the # here does not mean a comment)
#includedir <%= @config_dir %>
<% end -%>
